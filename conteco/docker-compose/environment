#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_STACKS="image.wrapper"
export CONTECO_DC_GLOBAL_STACKNAMES="image_wrapper"
export CONTECO_DC_GLOBAL_BOOTSTACKS=""
export CONTECO_DC_GLOBAL_RUNSTACKS="image_wrapper"
export CONTECO_DC_GLOBAL_VOLUME_LIST=""
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_IMAGE_WRAPPER_NAME,$CONTECO_DC_IMAGE_WRAPPER_ENVIRONMENT,$CONTECO_DC_IMAGE_WRAPPER_COMMAND,$CONTECO_DC_IMAGE_WRAPPER_VOLUMES,$CONTECO_DC_IMAGE_WRAPPER_NETWORKS,$CONTECO_DC_IMAGE_WRAPPER_PORTS,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_MODE,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_REPLICAS,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_CPUS,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_CPUS,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY,$CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_IMAGE_WRAPPER_ENVIRONMENT=
export CONTECO_DC_IMAGE_WRAPPER_COMMAND="'sleep 0.5'"
export CONTECO_DC_IMAGE_WRAPPER_PORTS=
export CONTECO_DC_IMAGE_WRAPPER_VOLUMES=
export CONTECO_DC_IMAGE_WRAPPER_NETWORKS=
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_LABELS=
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_MODE="replicated"
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_REPLICAS="1"
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_CPUS="0.75"
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_CPUS="0.0"
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export CONTECO_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START TOPLEVEL SETTINGS
export CONTECO_DC_GLOBAL_VOLUMES=
export CONTECO_DC_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
